#include <iostream>
using namespace std;
#include<bits/stdc++.h>
string longe(string s, int n)
{
    string e;
	    for(int i=0;i<n-1;i++)
	    {
	        string d="";
	        d=d+s[i];
	        for(int j=i+1;j<n;j++)
	        {
	            d=d+s[j];
	        }
	        e=d;
	        reverse(d.begin(),d.end());
	        if(e==d)
	        {
	            return e;
	        }
	    }
	    string g="";
	    g=g+s[0];
	    return g;
}
int main() {
	//code
		//code
	int t;
	cin>>t;
	while(t--)
	{
	    string s;
	    cin>>s;
	    int n=s.size();
	    string h;
	    h=longe(s,n);
	    cout<<h;   
	}
	return 0;
}
